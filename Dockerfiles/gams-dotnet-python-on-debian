FROM jasdefer/gams-dotnet-on-debian
RUN apt-get update -y &&\
	apt-get install python3.7 -y &&\
	apt-get install python3.7-distutils -y
RUN export PYTHONPATH=/gams/gams34.3_linux_x64_64_sfx/apifiles/Python/api_37 &&\
	export PYTHONPATH=/gams/gams34.3_linux_x64_64_sfx/apifiles/Python/gams:$PYTHONPATH
RUN python3.7 /gams/gams34.3_linux_x64_64_sfx/apifiles/Python/api_37/setup.py install