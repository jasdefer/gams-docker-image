FROM jasdefer/gams-dotnet-on-debian
RUN apt-get update -y &&\
	apt-get install python3 python3-pip -y &&\
	apt-get install python3-distutils -y
RUN export PYTHONPATH=/gams/gams38.1_linux_x64_64_sfx/apifiles/Python/api_39 &&\
	export PYTHONPATH=/gams/gams38.1_linux_x64_64_sfx/apifiles/Python/gams:$PYTHONPATH
RUN python3 /gams/gams38.1_linux_x64_64_sfx/apifiles/Python/api_39/setup.py install